    "express": "^4.16.2",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "pm2": "^4.5.6",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "routing-controllers": "^0.7.6",
    "typed-rest-client": "^1.8.4",
    "typedi": "^0.7.2",
    "winston": "^2.4.0"
  },
